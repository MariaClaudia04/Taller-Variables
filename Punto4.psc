Algoritmo Punto4
	definir nombreciudad Como Caracter;
	definir pais Como Caracter;
	
	escribir "¿como se llama la ciudad?"
	leer nombreciudad;
	
	escribir "¿como se llama el pais de la ciudad ?"
	leer pais;
	
	escribir " la ciudad de ", nombreciudad, " es una captal del pais de ", pais;
FinAlgoritmo
